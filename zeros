zeros.
..
package com.prabha.java;
public class Zeros {

	public static void main(String[] Args){
		int arr[]=new int[] {1,1,0,1,0,1,0};
		int temp[]= new int[arr.length];
		int j=0;
		for(int i=0;i<arr.length;i++)
		{
			if(arr[i]==1)
			{
				temp[j]=1;
				j++;
			} 
			
		}
			for(int array:temp) {
				System.out.println(array);
			}
			}

}..
output:
1
1
1
1
0
0
